{
  "name": "New Tab Blocker",
  "version": "1.0",
  "description": "Prevent page from creating new tab",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "helper.js",
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/favicon-16.png",
      "32": "images/favicon-32.png",
      "48": "images/favicon-48.png",
      "128": "images/favicon-128.png"
    }
  },
  "icons": {
    "16": "images/favicon-16.png",
    "32": "images/favicon-32.png",
    "48": "images/favicon-48.png",
    "128": "images/favicon-128.png"
  },
  "options_page": "options.html"
}

